{
  "name": "express-jwt-react",
  "version": "1.0.0",
  "description": "An express app with JWT authentication and React frontend",
  "main": "index.js",
  "author": "Sergei Samsonov <osenvosem@gmail.com> (https://github.com/osenvosem)",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec babel-node src/server.ts --extensions '.ts','.tsx'",
    "start:debug": "yarn start --inspect",
    "start:prod": "node dist/server.js",
    "build": "NODE_ENV=production babel src --ignore src/**/__tests__ --out-dir dist --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "@babel/core": "^7.0.0-beta.38",
    "@babel/node": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "@babel/preset-react": "^7.0.0-beta.38",
    "@babel/preset-typescript": "^7.0.0-beta.38",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.11.0",
    "@types/jest": "^22.1.0",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.96",
    "@types/mongodb": "^3.0.5",
    "@types/mongoose": "^4.7.33",
    "@types/node": "^9.3.0",
    "@types/passport": "^0.4.2",
    "@types/passport-jwt": "^3.0.0",
    "@types/serve-favicon": "^2.2.30",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.1.0",
    "jest": "^22.1.4",
    "nodemon": "^1.14.7"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "config": "^1.29.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.4",
    "mongoose": "^5.0.1",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "serve-favicon": "^2.4.5"
  }
}
